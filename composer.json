{
  "keywords": [
    "Php",
    "Slim 4",
    "Php Authentication",
    "Slim 4 Authentication"
  ],
  "name": "ichinya/slim_learn",
  "require": {
    "PHP": "^7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "slim/slim": "4.*",
    "slim/psr7": "^1.4",
    "php-di/php-di": "^6.3",
    "php-di/slim-bridge": "^3.1",
    "jenssegers/blade": "^1.4",
    "illuminate/support": "^8.49",
    "illuminate/database": "^8.49",
    "zeuxisoo/slim-whoops": "^0.7.3",
    "vlucas/phpdotenv": "*",
    "robmorgan/phinx": "^0.12",
    "fzaninotto/faker": "*",
    "slim/csrf": "^1.2.1",
    "illuminate/validation": "^8.50.0",
    "swiftmailer/swiftmailer": "^6.2",
    "guzzlehttp/guzzle": "^7.3.0",
    "illuminate/mail": "^8.50.0"
  },
  "autoload": {
    "files": [
      "./app/helpers.php"
    ],
    "classmap": [
      "./database/factories/support"
    ],
    "psr-4": {
      "App\\": "app",
      "Boot\\": "bootstrap"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "authors": [
    {
      "name": "Ichi",
      "email": "info@ichiblog.ru"
    }
  ]
}
